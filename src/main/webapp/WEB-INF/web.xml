<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">  
  <servlet>
    <servlet-name>AddShoppingCartServlet</servlet-name>
    <servlet-class>com.immutable.alias.servlet.AddShoppingCartServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetShoppingCartServlet</servlet-name>
    <servlet-class>com.immutable.alias.servlet.GetShoppingCartServlet</servlet-class>
  </servlet>
   <servlet-mapping>
    <servlet-name>AddShoppingCartServlet</servlet-name>
    <url-pattern>/servlet/addShoppingCart</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetShoppingCartServlet</servlet-name>
    <url-pattern>/servlet/getShoppingCart</url-pattern>
  </servlet-mapping>
 </web-app>
